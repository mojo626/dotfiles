(deflisten query :initial "" "tail -f /tmp/eww-launcher-query")
(deflisten results :initial "" "tail -f /tmp/eww-launcher-results")

(defwindow launcher  :screen 1 :monitor 0 :focusable "true"
  (box :orientation "vertical"
    (input
      :value query
      :onchange "echo '{}' >> /tmp/eww-launcher-query")
    (box
      (label :text {results}))))
